{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "valid_line": {
    "template": "\uf42e ",
    "foreground": "#00ff00",
    "background": "transparent"
  },
  "transient_prompt": {
    "template": " \uf101 ",
    "foreground": "#c0c0c0",
    "background": "transparent"
  },
  "error_line": {
    "template": "\ue009 ",
    "foreground": "#e91e63",
    "background": "transparent"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\uf0e7",
          "foreground": "#ffff66",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\uf460 ",
          "foreground": "#546E7A",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "<#546E7A>\u2026</>",
            "folder_separator_icon": "<#546E7A>/</>",
            "style": "folder"
          },
          "template": "{{ if .RootDir }}\ue271 {{ else if matchP \"^C:/Users/[^/]+$\" .Location }}\uf46d {{ else if matchP \"^\\\\.[^/]+$\" .Path }}\uf4c5 {{ else if glob \".git\\\\\\\\*\" }}<#f74e27>\ue65d</> {{ else if matchP \"^//[^/]+\" .Location }}\udb83\udc81 {{ else }}\uf115 {{ end }}",
          "foreground": "blue",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "<#546E7A>\u2026</>",
            "folder_separator_icon": "<#546E7A>/</>",
            "max_depth": 1,
            "style": "agnoster_short"
          },
          "template": "{{ .Path }}",
          "foreground": "cyan",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\uf460 ",
          "foreground": "#546E7A",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "branch_ahead_icon": "\uf176",
            "branch_behind_icon": "\uf175",
            "branch_icon": "\udb81\ude2c ",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "template": "{{ .UpstreamIcon }} ",
          "foreground": "#696969",
          "type": "git",
          "style": "plain",
          "foreground_templates": [
            "{{ if .Working.Changed }}#ffff00{{ end }}",
            "{{ if .Staging.Changed }}#3cb371{{end}}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff0000{{ end }}",
            "{{ if gt .Ahead 0 }}#9400d3{{ end }}",
            "{{ if gt .Behind 0 }}#1e90ff{{ end }}"
          ]
        },
        {
          "properties": {
            "branch_ahead_icon": "\uf176",
            "branch_behind_icon": "\uf175",
            "branch_icon": "\udb81\ude2c ",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "template": "{{ .HEAD }} ",
          "foreground": "#696969",
          "type": "git",
          "style": "plain",
          "foreground_templates": [
            "{{ if .Working.Changed }}#ffff00{{ end }}",
            "{{ if .Staging.Changed }}#3cb371{{end}}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff0000{{ end }}",
            "{{ if gt .Ahead 0 }}#9400d3{{ end }}",
            "{{ if gt .Behind 0 }}#1e90ff{{ end }}"
          ],
          "min_width": 62
        },
        {
          "properties": {
            "branch_ahead_icon": "\uf176",
            "branch_behind_icon": "\uf175",
            "branch_icon": "\udb81\ude2c ",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "template": "{{if .BranchStatus }}{{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf0c7 {{ .StashCount }}{{ end }}",
          "foreground": "#696969",
          "type": "git",
          "style": "plain",
          "foreground_templates": [
            "{{ if .Working.Changed }}#ffff00{{ end }}",
            "{{ if .Staging.Changed }}#3cb371{{end}}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff0000{{ end }}",
            "{{ if gt .Ahead 0 }}#9400d3{{ end }}",
            "{{ if gt .Behind 0 }}#1e90ff{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "overflow": "hide",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "display_exit_code": true
          },
          "template": "{{ if .Error }}{{ .String }} \uf40c{{ else }}\uf00c{{ end }}",
          "foreground": "#00ff00",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if .Error}}#e91e63{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \u2039",
          "foreground": "#546E7A",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " cmake {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue61e",
          "foreground": "#2cb25b",
          "type": "cmake",
          "style": "plain",
          "min_width": 62
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue61d ",
          "foreground": "#2cb25b",
          "type": "cmake",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2039",
          "foreground": "#546E7A",
          "type": "cmake",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "context"
          },
          "template": " {{ if .Venv }}{{ .Venv }}{{ end }}",
          "foreground": "#ffff00",
          "type": "python",
          "style": "plain",
          "min_width": 62
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "context"
          },
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#ffff00",
          "type": "python",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "context"
          },
          "template": " \udb80\udf20 ",
          "foreground": "#0068b7",
          "type": "python",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "context"
          },
          "template": "\u2039",
          "foreground": "#546E7A",
          "type": "python",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "mapped_shell_names": {
              "bash": "bash",
              "cmd": "Cmd",
              "pwsh": "Powershell"
            }
          },
          "template": " {{ .Name }}",
          "foreground": "#cdcdcd",
          "type": "shell",
          "style": "plain",
          "min_width": 62
        },
        {
          "properties": {
            "cache_duration": "none",
            "mapped_shell_names": {
              "bash": "\udb84\udd83",
              "cmd": "\uebc4",
              "pwsh": "\udb82\ude0a"
            }
          },
          "foreground": "#cdcdcd",
          "type": "shell",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2039",
          "foreground": "#546E7A",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "charged_icon": "\ue22f ",
            "charging_icon": "<f>\ue234 </>",
            "discharging_icon": "<f>\ue231 </>"
          },
          "template": " {{ if not .Error }}{{ .Percentage }}\udb80\udff0 {{ .Icon }}{{ end }}",
          "foreground": "#ffeb3b",
          "type": "battery",
          "style": "plain",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
            "{{if eq \"Discharging\" .State.String}}#ff5722{{end}}",
            "{{if eq \"Full\" .State.String}}#4caf50{{end}}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2039",
          "foreground": "#546E7A",
          "type": "battery",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "foreground": "#d0ecff",
          "type": "time",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2039",
          "foreground": "#546E7A",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "ubuntu": "\uf31b",
            "windows": "\ue70f"
          },
          "template": " {{ if .WSL }}WSL at{{ end }}",
          "foreground": "#00bfff",
          "type": "os",
          "style": "plain",
          "min_width": 62
        },
        {
          "properties": {
            "cache_duration": "none",
            "ubuntu": "\uf31b",
            "windows": "\ue70f"
          },
          "template": " {{.Icon}} ",
          "foreground": "#00bfff",
          "type": "os",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf101 ",
          "foreground": "#c0c0c0",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
